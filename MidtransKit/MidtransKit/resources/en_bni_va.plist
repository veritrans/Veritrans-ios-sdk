		<string>ATM BNI</string>
		<key>guides</key>
		<array>
			<dict>
				<key>content</key>
				<string>On the main menu, choose ‘Other Transaction’</string>
				<key>image</key>
				<string>bni_va1</string>
			</dict>
			<dict>
				<key>content</key>
				<string>Choose ‘Transfer’</string>
				<key>image</key>
				<string>bni_va2</string>
			</dict>
			<dict>
				<key>content</key>
				<string>Choose ‘Savings Account’</string>
				<key>image</key>
				<string>bni_va3</string>
			</dict>
			<dict>
				<key>content</key>
				<string>Choose ‘To BNI Account’</string>
				<key>image</key>
				<string>bni_va4</string>
			</dict>
			<dict>
				<key>content</key>
				<string>Enter ‘BNI virtual account number’ and press ‘Correct’</string>
				<key>image</key>
				<string>bni_va5</string>
			</dict>
			<dict>
				<key>content</key>
				<string>Enter the ‘full amount to be paid’ and press ‘Correct’</string>
				<key>image</key>
				<string>bni_va6</string>
			</dict>
			<dict>
				<key>content</key>
				<string>Your payment details will appear on the confirmation page. If the information is correct press ‘Correct’</string>
				<key>image</key>
				<string>bni_va7</string>
			</dict>
		</array>
	</dict>
	<dict>
		<key>name</key>
		<string>Mobile Banking</string>
		<key>guides</key>
		<array>
			<dict>
				<key>content</key>
				<string>Open the BNI Mobile Banking app and login</string>
				<key>image</key>
				<string></string>
			</dict>
			<dict>
				<key>content</key>
				<string>Click ‘Transfer’</string>
				<key>image</key>
				<string></string>
			</dict>
			<dict>
				<key>content</key>
				<string>Click ‘To BNI Account’</string>
				<key>image</key>
				<string></string>
			</dict>
			<dict>
				<key>content</key>
				<string>Click ‘Input New Account’</string>
				<key>image</key>
				<string></string>
			</dict>
			<dict>
				<key>content</key>
				<string>Enter account numbers, an then click ‘Continue’</string>
				<key>image</key>
				<string></string>
			</dict>
			<dict>
				<key>content</key>
				<string>Input the ‘Transaction Password’ and then click ‘Continue’ and you are done. </string>
				<key>image</key>
				<string></string>
			</dict>
		</array>
	</dict>
